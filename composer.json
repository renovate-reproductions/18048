{
    "name": "lullabot/lullabot.com-d8",
    "description": "Drupal site for www.lullabot.com",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "drupal/search_exclude_nid",
                "type": "drupal-module",
                "version": "1.2",
                "source": {
                    "type": "git",
                    "url": "https://git.drupalcode.org/project/search_exclude_nid.git",
                    "reference": "fd997ee8e7cba2fa955c2d82986f9d85e30d5384"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "drupal/layout_discovery_override",
                "type": "drupal-module",
                "version": "1.0",
                "source": {
                    "type": "git",
                    "url": "https://git.drupalcode.org/project/layout_discovery_override.git",
                    "reference": "a883c7731f962067c7ac0fafc18dfee40baf532d"
                }
            }
        },
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    ],
    "require": {
        "php": "^7.4",
        "composer/composer": "^2.2.16",
        "composer/installers": "^2.1.1",
        "cweagans/composer-patches": "^1.7.2",
        "drupal/access_unpublished": "^1.4",
        "drupal/admin_toolbar": "^3.1.1",
        "drupal/allowed_formats": "^1.5",
        "drupal/antibot": "^2.0",
        "drupal/auditfiles": "^3.0@beta",
        "drupal/auto_entitylabel": "^3-beta",
        "drupal/block_blacklist": "^1.1",
        "drupal/block_class": "^2.0.3",
        "drupal/block_list_override": "^1.0.1",
        "drupal/bricks": "^2.0.3",
        "drupal/chosen": "^3.0.3",
        "drupal/ckeditor_markdown": "^1.3",
        "drupal/ckeditor_quote": "^1.0.0@alpha",
        "drupal/ckeditorheight": "^1.8",
        "drupal/codesnippet": "^1.7",
        "drupal/config_filter": "^1.10",
        "drupal/config_ignore": "^2.3",
        "drupal/config_override_warn": "^1.3",
        "drupal/config_split": "^2.0.0-rc3@beta",
        "drupal/core-composer-scaffold": "^9.4.5",
        "drupal/core-recommended": "^9.4.5",
        "drupal/ctools": "^4.0.1",
        "drupal/devel": "^4.1.5",
        "drupal/diff": "^1.0.0@RC",
        "drupal/dropzonejs": "^2.7",
        "drupal/eck": "^1.0.0-beta2@beta",
        "drupal/embed": "^1.6",
        "drupal/entity_browser": "^2.8",
        "drupal/entity_browser_block": "^1.3",
        "drupal/entity_embed": "^1.2.0@beta",
        "drupal/entity_usage": "2.0.0-beta7@alpha",
        "drupal/entityqueue": "^1.2",
        "drupal/environment_indicator": "^4.0.9",
        "drupal/eu_cookie_compliance": "^1.21",
        "drupal/externalauth": "^2.0.2",
        "drupal/field_group": "^3.2",
        "drupal/honeypot": "^2.1.1",
        "drupal/htmlpurifier": "^1.0.0-rc2@RC",
        "drupal/http2_server_push": "^1.1",
        "drupal/inline_entity_form": "^1.0.0-rc9@RC",
        "drupal/layout_builder_modal": "^1.1",
        "drupal/layout_discovery_override": "^1.0",
        "drupal/mailchimp": "^2.0.2",
        "drupal/markdown": "^3",
        "drupal/maxlength": "^2.0.0@RC",
        "drupal/media_entity_browser": "^2.0-alpha",
        "drupal/menu_trail_by_path": "^1.3",
        "drupal/metatag": "^1.21",
        "drupal/pathauto": "^1.11",
        "drupal/podcast": "^1.2",
        "drupal/protected_pages": "^1.4",
        "drupal/quicklink": "^2.0.1@beta",
        "drupal/rabbit_hole": "^1.0",
        "drupal/redirect": "^1.8",
        "drupal/redis": "^1.5",
        "drupal/require_on_publish": "^1.6.0@beta",
        "drupal/scheduler": "^1.4",
        "drupal/schema_metatag": "^2.3",
        "drupal/search404": "^2.0",
        "drupal/search_exclude": "^2.0-beta",
        "drupal/search_exclude_nid": "^1.2",
        "drupal/sendgrid_integration": "^1.7",
        "drupal/simple_ldap": "^1.0-alpha",
        "drupal/smtp": "^1.1@beta",
        "drupal/smtp_multiple": "^1.0",
        "drupal/stage_file_proxy": "^1.2.0@alpha",
        "drupal/subpathauto": "^1.2.0@beta",
        "drupal/token": "^1.11",
        "drupal/transliterate_filenames": "^2.0.1",
        "drupal/twig_tweak": "^3.1.3",
        "drupal/type_tray": "^1.1.1",
        "drupal/tzfield": "^1.6",
        "drupal/views_autocomplete_filters": "^1.3",
        "drupal/views_bulk_edit": "^2.6",
        "drupal/views_bulk_operations": "^4.1.4",
        "drupal/views_layout": "^1.2",
        "drupal/warmer": "^2.0.7",
        "drupal/webform": "^6.1.3",
        "drupal/webform_config_ignore": "^1.2",
        "drupal/webform_mailchimp": "^5.9",
        "drupal/webp": "^1.0@beta",
        "drupal/xmlsitemap": "^1.3",
        "drush/drush": "^10.6.2",
        "lullabot/drainpipe": "^1.2.16",
        "phpunit/phpunit": "^9.5.24"
    },
    "require-dev": {
        "behat/mink": "^1.10",
        "behat/mink-selenium2-driver": "^1.6",
        "consolidation/robo": "^1.5",
        "davidrjonas/composer-lock-diff": "^1.7",
        "drupal/hacked": "2.x-dev@dev",
        "drupal/security_review": "^1.0.0-alpha2@alpha",
        "drupal/site_audit": "^3.0@RC",
        "drupal/unused_modules": "^1.3",
        "drupal/upgrade_status": "^3.17",
        "kint-php/kint": "^4.1.2",
        "lullabot/drainpipe-dev": "^2.1",
        "mglaman/drupal-check": "^1.4",
        "vlucas/phpdotenv": "^3.6.10",
        "weitzman/drupal-test-traits": "^1.6"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "ext-ldap": "^7.2.0",
        "sort-packages": true,
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "cweagans/composer-patches": true,
            "drupal/core-composer-scaffold": true,
            "drupal/core-project-message": true,
            "drupal/core-vendor-hardening": true,
            "composer/installers": true,
            "lullabot/drainpipe": true,
            "ergebnis/composer-normalize": true,
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "lullabot/drainpipe-dev": true,
            "zaporylie/composer-drupal-optimizations": true,
            "oomphinc/composer-installers-extender": true
        }
    },
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ],
        "psr-4": {
            "Lullabot\\": ["src/"]
        }
    },
    "autoload-dev": {
        "files": [
            "load.environment.php"
        ]
    },
    "extra": {
        "drainpipe": {
            "github": ["PantheonReviewApps"]
        },
        "drupal-scaffold": {
            "gitignore": "true",
            "allowed-packages": ["lullabot/drainpipe-dev"],
            "locations": {
                "web-root": "web/"
            },
            "file-mapping": {
                "[project-root]/test/nightwatch/example.nightwatch.js": {
                    "mode": "skip"
                },
                "[project-root]/.gitattributes": {
                    "append": "scaffold/gitattributes"
                }
            }
        },
        "installer-paths": {
            "web/core": [
                "type:drupal-core"
            ],
            "web/libraries/{$name}": [
                "type:drupal-library"
            ],
            "web/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "web/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "web/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/Commands/contrib/{$name}": [
                "type:drupal-drush"
            ],
            "web/modules/custom/{$name}": [
                "type:drupal-custom-module"
            ],
            "web/profiles/custom/{$name}": [
                "type:drupal-custom-profile"
            ],
            "web/themes/custom/{$name}": [
                "type:drupal-custom-theme"
            ]
        },
        "patches": {
            "drupal/core": {
                "Issue #2924204: Add <image> element to RSS channel": "https://www.drupal.org/files/issues/2021-11-22/rss-feed-image-2924204-30.patch",
                "Issue #3202896: Don't display OEmbed error to anonymous visitors when resource stops being available": "https://www.drupal.org/files/issues/2021-10-22/issue-3202896-5.patch",
                "Issue #3056633: Drupal 9 updating issue - Drupal\\Component\\Plugin\\Exception\\PluginNotFoundException: The <whatever> plugin does not exist": "patches/3056633-147.patch"
            },
            "drupal/entity_embed": {
                "Issue #3077225: Logs flooded with warning messages 'Invalid display settings encountered'": "https://www.drupal.org/files/issues/2019-12-11/3077225-10.reduce-invalid-config-logs.patch"
            },
            "drupal/entity_browser": {
                "Issue #3008700: Prevent 'auto_open' infinite loop": "https://www.drupal.org/files/issues/2018-10-23/entity_browser-prevent-auto-open-infinite-loop-3008700-2.patch"
            },
            "drupal/simple_ldap": {
                "PHP 7.4: Array and string offset access syntax with curly braces is deprecated": "https://www.drupal.org/files/issues/2020-06-16/bugfix-php-7.4-curly-braces-method_3151212_10_0.patch",
                "Issue #3240127: Fix userIdFromLdapUser typo": "https://www.drupal.org/files/issues/2021-09-30/query-with-or.patch"
            },
            "weitzman/drupal-test-traits": {
                "Register custom namespaces so we can use bootstrap-fast": "patches/register-namespaces-in-bootstrap-fast.patch"
            },
            "drupal/search_exclude_nid": {
                "Issue #2997317: Warning in PHP 7.2 when no nids configured": "https://www.drupal.org/files/issues/2018-09-04/count-warning-2997317.patch",
                "Automated Drupal 9 compatibility fixes": "https://www.drupal.org/files/issues/2021-06-15/3148714-7.patch"
            },
            "drupal/layout_discovery_override": {
                "Drupal 9 compatibility": "https://www.drupal.org/files/issues/2020-05-21/3138546-4.patch"
            }
        },
        "composer-exit-on-patch-failure": true
    }
}
